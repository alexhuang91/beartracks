%h1 Resident Login

= form_for @resident_session, :url => {:action => :create} do |f|
  
  = f.label :login
  %br
  = f.text_field :login
  %br
  
  = f.label :password
  %br
  = f.password_field :password
  %br
  
  = f.check_box :remember_me
  = f.label :remember_me
  
  %br
  
  = f.submit "Login"
